
package rommel;

public class Array {
    public static void main(String[] args) {
        System.out.println("List of animals: ");
        System.out.println("---------------");
     String[] animls = {"dragon","dog","cat","zebra","lion"};
     int[] myArray = {-1,-2,3,0,5,6,7,9,10,11,13,15,-10,-14,4};
     
        for (int d = 4; d >= 0; d--){ 
            System.out.println(animls[d]);

        }
            System.out.println("--------------- ");
            System.out.println("List of Numbers: ");
        for (int z = 14; z >= 0; z--){
            System.out.println(myArray[z]);
        }
    
}
}
