 Using For Loops
print("2 to 10")
for x in range (2,12,2):
    print(x)

print("3 to 12")
for y in range (3,15,3):
    print (y)

print("4 to 20")
for z in range(4,24,4):
        print(z)

print("12 to 36")
for z in range(12,40,12):
        print(z)
print("multiplication ")
for w in range(1,11):
    for z in range(1,11):
        print(z * w,end='\t')
    print()

print("multiplication by 10")
for y in range(10,21):
    for z in range(1,11):
        print(z * y,end='\t')
    print()
